{
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://adriandelr.github.io/portfolio",
  "scripts": {
    "deploy": "gh-pages -d web-build",
    "predeploy": "expo build:web",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && expo start",
    "android": "export NODE_OPTIONS=--openssl-legacy-provider && expo start --android",
    "ios": "export NODE_OPTIONS=--openssl-legacy-provider && expo start --ios",
    "web": "export NODE_OPTIONS=--openssl-legacy-provider && expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "eas-install": "yarn add -g eas-cli",
    "eas-login": "eas login",
    "eas-build-android": "eas build --platform android",
    "eas-build-ios": "eas build --platform ios",
    "eas-build-all": "eas build --platform all",
    "update-peer-deps-masked-view": "yarn add react-native-reanimated react-native-gesture-handler react-native-screens react-native-safe-area-context @react-native-community/masked-view --legacy-peer-deps",
    "update-peer-deps-nav-stack": "yarn add @react-navigation/stack --legacy-peer-deps"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@expo/vector-icons": "^12.0.0",
    "@expo/webpack-config": "~0.12.45",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.1",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/material-top-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.9",
    "@react-navigation/stack": "^5.14.9",
    "@types/react-native-snap-carousel": "^3.8.2",
    "expo": "^40.0.0",
    "expo-asset": "~8.2.1",
    "expo-constants": "~9.3.3",
    "expo-font": "~8.4.0",
    "expo-image": "^1.0.0-alpha.0",
    "expo-linking": "~2.0.1",
    "expo-splash-screen": "~0.8.1",
    "expo-status-bar": "~1.0.3",
    "expo-web-browser": "~8.6.0",
    "lodash": "^4.17.20",
    "modal-react-native-web": "^0.2.0",
    "react": "16.13.1",
    "react-device-detect": "^1.15.0",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-elements": "^3.0.1",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-read-more-text": "^1.1.2",
    "react-native-reanimated": "~1.13.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "~0.13.12",
    "react-native-x2-carousel": "^1.0.1",
    "react-qr-code": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@tsconfig/react-native": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.168",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-native-read-more-text": "^1.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-test-renderer": "^18.0.7",
    "gh-pages": "^3.1.0",
    "jest-expo": "^40.0.0",
    "typescript": "~4.0.0"
  },
  "private": true
}
